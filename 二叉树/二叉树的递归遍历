public class Main {
    public void preOrder(TreeNode root) {
    	if(root!=null)
    	{
    		System.out.println(root.val);
        	preOrder(root.left);
        	preOrder(root.right);
    	}
    }
    
    public void inOrder(TreeNode root) {
    	if(root!=null)
    	{
    		inOrder(root.left);
    		System.out.println(root.val);
    		inOrder(root.right);
    	}
    }
    
    public void postOrder(TreeNode root) {
    	if(root!=null)
    	{
    		postOrder(root.left);
    		postOrder(root.right);
        	System.out.println(root.val);
    	}
    }

    public static void main(String[] args)
    {
    	//Main s = new Main();

    }
}